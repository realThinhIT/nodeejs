{
  "name": "node-express-rest-api-starter-pack",
  "version": "1.0.0",
  "description": "Starter pack to build RESTful API with NodeJS + ExpressJS + Mongoose",
  "main": "server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "forever -w server-dev.js",
    "clean...": "rm -rf build && mkdir build",
    "clean": "rmdir build /s /q && mkdir build",
    "build-server": "babel ./ -s -D -d ./build --ignore node_modules,node_modules/*",
    "build": "npm run clean && npm run build-server",
    "start": "forever start ./build/server.js"
  },
  "author": "ThinhIT (thinhit.net)",
  "license": "MIT",
  "dependencies": {
    "babel-cli": "^6.18.0",
    "babel-plugin-transform-remove-strict-mode": "0.0.2",
    "babel-preset-es2015": "^6.18.0",
    "babel-register": "^6.18.0",
    "bluebird": "^3.4.6",
    "body-parser": "^1.15.2",
    "dot-object": "^1.5.4",
    "express": "^4.14.0",
    "glob": "^7.1.0",
    "md5": "^2.2.1",
    "mongodb": "^2.2.10",
    "mongoose": "^4.6.6",
    "morgan": "^1.7.0",
    "nodemon": "^1.11.0",
    "path": "^0.12.7"
  }
}
