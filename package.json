{
  "name": "nodeejs",
  "version": "1.0.0",
  "description": "NodeeJS - Starter pack to build RESTful API with NodeJS + ExpressJS + Mongoose",
  "main": "server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "forever -w server-dev.js",
    "clean...": "rm -rf build && mkdir build",
    "clean": "rm -rf build && mkdir build",
    "build-server": "babel ./ -s -D -d ./build --ignore node_modules,node_modules/*",
    "build": "npm run clean && npm run build-server",
    "start": "forever start ./build/server.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/realThinhIT/nodeejs.git"
  },
  "author": "ThinhIT (thinhit.net)",
  "license": "MIT",
  "dependencies": {
    "babel-cli": "^6.18.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-remove-strict-mode": "0.0.2",
    "babel-plugin-transform-strict-mode": "^6.18.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-register": "^6.18.0",
    "bluebird": "^3.4.6",
    "body-parser": "^1.15.2",
    "cors": "^2.8.1",
    "dot-object": "^1.5.4",
    "express": "^4.16.2",
    "express-handlebars": "^3.0.0",
    "forever": "^0.15.3",
    "glob": "^7.1.0",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^4.1.0",
    "gulp-cssnano": "^2.1.2",
    "gulp-nodemon": "^2.2.1",
    "gulp-notify": "^3.1.0",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "^3.1.0",
    "lodash": "^4.17.4",
    "md5": "^2.2.1",
    "mongodb": "^2.2.10",
    "mongoose": "^4.11.11",
    "morgan": "^1.7.0",
    "mysql": "^2.15.0",
    "mysql2": "^1.5.1",
    "node-sass": "^4.7.2",
    "nodemon": "^1.11.0",
    "path": "^0.12.7",
    "request": "^2.81.0",
    "sequelize": "^4.29.2"
  },
  "standard": {
    "parser": "babel-eslint"
  },
  "devDependencies": {
    "babel-eslint": "^8.0.0"
  }
}
